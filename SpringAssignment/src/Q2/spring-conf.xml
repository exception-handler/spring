<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<!--
	constructor  dependency injection 
	  -->
	<bean class="Q2.Employee" id="emp1" >
	<constructor-arg name="id" value="101" type="int"></constructor-arg>
	<constructor-arg name="name" value="Rohit"></constructor-arg>
	<constructor-arg name="salary" value="1000000" type="float"></constructor-arg>
	</bean>

 
</beans>